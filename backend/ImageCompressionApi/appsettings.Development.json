{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Information",
        "System": "Information",
        "ImageCompressionApi": "Debug"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "ImageCompressionApi": "Debug",
      "System": "Information"
    }
  },
  "ImageCompression": {
    "TempFileRetentionMinutes": 10,
    "FFmpegPath": "",
    "MaxConcurrentOperations": 2,
    "FFmpegTimeoutSeconds": 60
  },
  "Cors": {
    "AllowedOrigins": ["*"],
    "AllowedMethods": ["*"],
    "AllowedHeaders": ["*"],
    "AllowCredentials": false
  }
} 